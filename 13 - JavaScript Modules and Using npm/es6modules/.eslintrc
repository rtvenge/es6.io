{
  "env": {
    "es6": true,
    "browser": true
  },
  "extends": "airbnb",
  "rules": {
    "comma-dangle": [
      1,
      "never"
    ],
    "quotes": 0,
    "linebreak-style": [
      2,
      "unix"
    ],
    "semi": [
      2,
      "always"
    ],
    "no-console": 0,
    "no-unused-vars": 1
  },
  "globals": {
    "jQuery": true
  }
}
